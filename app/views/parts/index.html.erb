<p id="notice"><%= notice %></p>

<h1>Parts</h1>

<table>
  <thead>
    <tr>
      <th>Partnum</th>
      <th>Description</th>
      <th>Quanitity</th>
      <th>Totalquantity</th>
      <th>Workorder</th>
      <th>Finish</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parts.each do |part| %>
      <tr>
        <td><%= part.partNum %></td>
        <td><%= part.description %></td>
        <td><%= part.quanitity %></td>
        <td><%= part.totalQuantity %></td>
        <td><%= part.workOrder %></td>
        <td><%= part.finish %></td>
        <td><%= link_to 'Show', part %></td>
        <td><%= link_to 'Edit', edit_part_path(part) %></td>
        <td><%= link_to 'Destroy', part, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Part', new_part_path %>
